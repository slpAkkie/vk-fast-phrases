{
    "name": "funButtons",
    "version": "1.0.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://vk.com/*"
            ],
            "js": [
                "content_scripts/background.js",
                "content_scripts/chrome_messages.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "Кнопоцка",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "storage"
    ]
}
